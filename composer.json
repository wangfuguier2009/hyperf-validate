{
  "name": "wangdeguier/hyperf-validate",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "framework",
    "hyperf"
  ],
  "description": "基于mzh/hyperf-validate适配项目",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.3",
    "ext-swoole": ">=4.5",
    "hyperf/http-message": "~2.1.1",
    "hyperf/config": "~2.1.0",
    "hyperf/di": "~2.1.0",
    "hyperf/framework": "~2.1.0",
    "hyperf/utils": "~2.1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "suggest": {
    "ext-json": "Required to use JSON."
  },
  "autoload": {
    "psr-4": {
      "Mzh\\Validate\\": "src/"
    }
  },
  "extra": {
    "hyperf": {
      "config": "Mzh\\Validate\\ConfigProvider"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
